import { default as React } from 'react';
declare const _default: {
    title: string;
    component: React.FC<import('./FormHint.component').FormHintProps>;
    argTypes: {
        children: {
            control: boolean;
            table: {
                type: {
                    summary: string;
                };
            };
        };
    };
};
export default _default;
type TemplateProps = {
    children: React.ReactNode;
};
export declare const Default: {
    render: ({ children, ...args }: TemplateProps) => React.JSX.Element;
    args: {
        text: string;
    };
};
export declare const WithChildren: {
    render: ({ children, ...args }: TemplateProps) => React.JSX.Element;
    args: {
        children: React.JSX.Element;
    };
};
export declare const WithTextAsChildren: {
    render: ({ children, ...args }: TemplateProps) => React.JSX.Element;
    args: {
        text: React.JSX.Element;
    };
};
export declare const ErrorVariant: {
    render: ({ children, ...args }: TemplateProps) => React.JSX.Element;
    args: {
        variant: string;
        text: string;
    };
};
export declare const SuccessVariant: {
    render: ({ children, ...args }: TemplateProps) => React.JSX.Element;
    args: {
        variant: string;
        text: string;
    };
};
