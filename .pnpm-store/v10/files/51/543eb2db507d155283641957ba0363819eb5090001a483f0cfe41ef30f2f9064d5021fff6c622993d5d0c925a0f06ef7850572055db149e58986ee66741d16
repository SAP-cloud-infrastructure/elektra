import { default as React } from 'react';
declare const _default: {
    title: string;
    component: React.FC<import('./RadioGroup.component').RadioGroupProps>;
    argTypes: {
        items: {
            table: {
                disable: boolean;
            };
        };
        errortext: {
            control: boolean;
        };
        helptext: {
            control: boolean;
        };
        successtext: {
            control: boolean;
        };
        children: {
            control: boolean;
        };
        onChange: {
            table: {
                type: {
                    summary: string;
                };
            };
        };
    };
};
export default _default;
interface TemplateProps {
    children: React.ReactNode;
}
