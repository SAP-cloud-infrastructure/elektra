import { Meta, StoryObj } from '@storybook/react-vite';
import { Stack } from './Stack.component';
declare const meta: Meta<typeof Stack>;
export default meta;
type Story = StoryObj<typeof meta>;
export declare const Horizontal: Story;
export declare const Vertical: Story;
export declare const GapHorizontal: Story;
export declare const GapVertical: Story;
export declare const HorizontalAlignmentCenter: Story;
export declare const VerticalAlignmentCenter: Story;
export declare const HorizontalAlignmentStart: Story;
export declare const VerticalAlignmentStart: Story;
export declare const HorizontalDistributionCenter: Story;
export declare const VerticalDistributionCenter: Story;
export declare const Centered: Story;
export declare const Wrap: Story;
