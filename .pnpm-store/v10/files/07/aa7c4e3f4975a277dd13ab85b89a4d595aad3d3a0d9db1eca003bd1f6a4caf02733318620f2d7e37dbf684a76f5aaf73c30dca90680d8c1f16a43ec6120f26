import { default as React } from 'react';
import { DataGridProps } from '../DataGrid/index';
type StoryFunction = () => JSX.Element;
declare const _default: {
    title: string;
    component: React.ForwardRefExoticComponent<import('./DataGridRow.component').DataGridRowProps & React.RefAttributes<HTMLDivElement>>;
    argTypes: {
        items: {
            table: {
                disable: boolean;
            };
        };
        children: {
            control: boolean;
            table: {
                type: {
                    summary: string;
                };
            };
        };
    };
    decorators: ((story: StoryFunction) => React.JSX.Element)[];
    parameters: {
        docs: {
            source: {
                excludeDecorators: boolean;
            };
        };
    };
};
export default _default;
interface TemplateProps {
    items: DataGridProps[];
}
export declare const Default: {
    render: ({ items, ...args }: TemplateProps) => React.JSX.Element;
    parameters: {
        docs: {
            description: {
                story: string;
            };
        };
    };
    args: {
        items: any[];
    };
};
