import { default as React } from 'react';
declare const _default: {
    title: string;
    component: React.FC<import('./Radio.component').RadioProps>;
    argTypes: {
        errortext: {
            control: boolean;
        };
        helptext: {
            control: boolean;
        };
        successtext: {
            control: boolean;
        };
    };
};
export default _default;
export declare const Default: {
    args: {
        value: string;
    };
};
export declare const Checked: {
    args: {
        checked: boolean;
    };
};
export declare const WithLabel: {
    args: {
        id: string;
        label: string;
    };
};
export declare const Required: {
    args: {
        required: boolean;
        label: string;
        helptext: string;
    };
};
export declare const WithHelptext: {
    args: {
        label: string;
        helptext: string;
    };
};
export declare const WithHelpTextAsNode: {
    args: {
        label: string;
        helptext: React.JSX.Element;
    };
};
export declare const Disabled: {
    args: {
        checked: boolean;
        disabled: boolean;
        label: string;
    };
};
export declare const Valid: {
    args: {
        valid: boolean;
        label: string;
    };
};
export declare const Invalid: {
    args: {
        invalid: boolean;
        label: string;
    };
};
export declare const ValidWithLabel: {
    args: {
        valid: boolean;
        label: string;
        successtext: string;
        helptext: string;
    };
};
export declare const InvalidWithLabel: {
    args: {
        invalid: boolean;
        label: string;
        helptext: string;
        errortext: string;
    };
};
