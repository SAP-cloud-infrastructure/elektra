import { default as React } from 'react';
import { SelectOptionProps } from './SelectOption.component';
declare const _default: {
    title: string;
    component: React.FC<SelectOptionProps>;
    argTypes: {
        children: {
            control: {
                type: string;
            };
            table: {
                type: {
                    summary: string;
                };
            };
        };
    };
    decorators: ((story: () => null | undefined) => React.JSX.Element)[];
};
export default _default;
export declare const Default: {
    render: (args: React.JSX.IntrinsicAttributes & SelectOptionProps) => React.JSX.Element;
    args: {
        value: string;
    };
};
export declare const Disabled: {
    render: (args: React.JSX.IntrinsicAttributes & SelectOptionProps) => React.JSX.Element;
    args: {
        disabled: boolean;
        value: string;
    };
};
export declare const ChildrenOnly: {
    render: (args: React.JSX.IntrinsicAttributes & SelectOptionProps) => React.JSX.Element;
    args: {
        children: string[];
    };
};
export declare const withLabel: {
    render: (args: React.JSX.IntrinsicAttributes & SelectOptionProps) => React.JSX.Element;
    args: {
        value: string;
        label: string;
    };
};
