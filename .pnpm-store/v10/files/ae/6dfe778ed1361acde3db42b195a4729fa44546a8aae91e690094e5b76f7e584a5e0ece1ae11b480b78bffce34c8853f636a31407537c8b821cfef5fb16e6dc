function hl(t, n) {
  let e = ge({ left: 0, top: 0 });
  if (Se(() => {
    if (!n) return;
    let o = n.getBoundingClientRect();
    o && (e.current = o);
  }, [t, n]), n == null || !t || n === document.activeElement) return !1;
  let r = n.getBoundingClientRect();
  return r.top !== e.current.top || r.left !== e.current.left;
}
let _i = /([\u2700-\u27BF]|[\uE000-\uF8FF]|\uD83C[\uDC00-\uDFFF]|\uD83D[\uDC00-\uDFFF]|[\u2011-\u26FF]|\uD83E[\uDD10-\uDDFF])/g;
function Hi(t) {
  var n, e;
  let r = (n = t.innerText) != null ? n : "", o = t.cloneNode(!0);
  if (!(o instanceof HTMLElement)) return r;
  let i = !1;
  for (let l of o.querySelectorAll('[hidden],[aria-hidden],[role="img"]')) l.remove(), i = !0;
  let a = i ? (e = o.innerText) != null ? e : "" : r;
  return _i.test(a) && (a = a.replace(_i, "")), a;
}
function Up(t) {
  let n = t.getAttribute("aria-label");
  if (typeof n == "string") return n.trim();
  let e = t.getAttribute("aria-labelledby");
  if (e) {
    let r = e.split(" ").map((o) => {
      let i = document.getElementById(o);
      if (i) {
        let a = i.getAttribute("aria-label");
        return typeof a == "string" ? a.trim() : Hi(i).trim();
      }
      return null;
    }).filter(Boolean);
    if (r.length > 0) return r.join(", ");
  }
  return Hi(t).trim();
}
function vl(t) {
  let n = ge(""), e = ge("");
  return Y(() => {
    let r = t.current;
    if (!r) return "";
    let o = r.innerText;
    if (n.current === o) return e.current;
    let i = Up(r).trim().toLowerCase();
    return n.current = o, e.current = i, i;
