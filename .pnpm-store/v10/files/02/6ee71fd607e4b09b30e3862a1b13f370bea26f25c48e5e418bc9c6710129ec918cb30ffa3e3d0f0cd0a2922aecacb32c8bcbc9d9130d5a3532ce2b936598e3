import { default as React } from 'react';
export declare const DataGridRow: React.ForwardRefExoticComponent<DataGridRowProps & React.RefAttributes<HTMLDivElement>>;
export interface DataGridRowProps extends React.HTMLAttributes<HTMLDivElement> {
    /** Children to render in the DataGridRow */
    children?: React.ReactNode;
    /** Add a classname */
    className?: string;
}
