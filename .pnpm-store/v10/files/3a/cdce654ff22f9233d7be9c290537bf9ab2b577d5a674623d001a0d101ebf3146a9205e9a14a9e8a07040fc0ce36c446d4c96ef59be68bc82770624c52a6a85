export declare const IconOnlyButton: ButtonStory;
export declare const IconOnlyAsChild: ButtonStory;
export declare const LinkAsButton: ButtonStory;
export declare const LinkAsButtonWithIcon: ButtonStory;
export declare const DefaultButtonInProgress: ButtonStory;
export declare const IconButtonInProgress: ButtonStory;
export declare const DefaultButtonInProgressWithProgressLabel: ButtonStory;
