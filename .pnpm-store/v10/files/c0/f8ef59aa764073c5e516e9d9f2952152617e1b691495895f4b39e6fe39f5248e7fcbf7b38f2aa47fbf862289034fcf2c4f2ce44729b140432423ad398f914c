import { default as React } from 'react';
import { DataGridHeadCellProps } from './index';
type StoryFunction = () => JSX.Element;
declare const _default: {
    title: string;
    component: React.ForwardRefExoticComponent<DataGridHeadCellProps & React.RefAttributes<HTMLDivElement>>;
    argTypes: {
        children: {
            control: boolean;
            table: {
                type: {
                    summary: string;
                };
            };
        };
    };
    decorators: ((story: StoryFunction) => React.JSX.Element)[];
    parameters: {
        docs: {
            source: {
                excludeDecorators: boolean;
            };
        };
    };
};
export default _default;
export declare const Default: {
    render: (args: DataGridHeadCellProps) => React.JSX.Element;
    parameters: {
        docs: {
            description: {
                story: string;
            };
        };
    };
    args: {
        children: string[];
    };
};
export declare const NoWrap: {
    render: (args: DataGridHeadCellProps) => React.JSX.Element;
    parameters: {
        docs: {
            description: {
                story: string;
            };
        };
    };
    args: {
        nowrap: boolean;
        children: string[];
    };
};
export declare const ColSpan: {
    render: (args: DataGridHeadCellProps) => React.JSX.Element;
    parameters: {
        docs: {
            description: {
                story: string;
            };
        };
    };
    args: {
        colSpan: number;
        children: string[];
    };
};
