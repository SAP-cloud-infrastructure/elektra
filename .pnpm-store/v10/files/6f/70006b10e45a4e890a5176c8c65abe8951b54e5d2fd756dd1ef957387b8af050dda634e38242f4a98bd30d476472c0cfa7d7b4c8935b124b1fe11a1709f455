import { default as React } from 'react';
import { TabProps } from '../Tab/Tab.component';
import { TabPanelProps } from '../TabPanel/TabPanel.component';
declare const _default: {
    title: string;
    component: {
        ({ children, defaultIndex, selectedIndex, onSelect, variant, className, ...props }: import('./Tabs.component').TabsProps): React.JSX.Element;
        tabsRole: string;
    };
    argTypes: {
        variant: {
            options: string[];
            control: {
                type: string;
            };
        };
        children: {
            control: boolean;
            table: {
                type: {
                    summary: string;
                };
            };
        };
        tabs: {
            table: {
                disable: boolean;
            };
        };
        tabpanels: {
            table: {
                disable: boolean;
            };
        };
    };
};
export default _default;
interface TemplateProps {
    tabs?: React.ReactElement<TabProps> | React.ReactElement<TabProps>[];
    tabpanels?: React.ReactElement<TabPanelProps> | React.ReactElement<TabPanelProps>[];
}
type OnSelectHandler = (value: number) => void;
interface ControlledTemplateProps {
    selectedIndex?: number;
    onSelect?: OnSelectHandler;
    tabs?: React.ReactElement<TabProps> | React.ReactElement<TabProps>[];
    tabpanels?: React.ReactElement<TabPanelProps> | React.ReactElement<TabPanelProps>[];
}
export declare const Default: {
    render: ({ tabs, tabpanels, ...args }: TemplateProps) => React.JSX.Element;
    args: {
        tabs: React.JSX.Element[];
        tabpanels: React.JSX.Element[];
    };
};
export declare const TabsWithIcons: {
    render: ({ tabs, tabpanels, ...args }: TemplateProps) => React.JSX.Element;
    args: {
        tabs: React.JSX.Element[];
        tabpanels: React.JSX.Element[];
    };
};
export declare const ControlledTabs: {
    render: ({ selectedIndex, onSelect, tabs, tabpanels, ...args }: ControlledTemplateProps) => React.JSX.Element;
    args: {
        tabs: React.JSX.Element[];
        tabpanels: React.JSX.Element[];
        selectedIndex: number;
        onSelect: () => void;
    };
};
