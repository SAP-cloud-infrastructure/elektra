%h3
%p
  = link_to 'pretty formatted', "#pretty_formatted", data: { toggle: "collapse" }
  .collapse#pretty_formatted
    %table.table
      %tbody
        - inquiry.payload.each do |k,v|
          %tr
            %th= k rescue ''
            %td= simple_format(v) rescue ''

%pre= JSON.pretty_generate(inquiry.payload) rescue "Error during rendering of JSON payload!"
